package miniProje1;

public class MiniProjeAsalSayi {
	public static void main(String[] args) {
		int sayi = 25;
		boolean asalmi = true;
		for (int i = 2; i < sayi; i++) {
	
			if (sayi % i == 0) {
				asalmi = false;

				System.out.println("Sayi Asaldir");
			}
			if (asalmi) {
				System.out.println("Sayi asal deÄŸilidr");
			}

		}

	}

}
