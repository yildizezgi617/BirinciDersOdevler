package miniProje1;

public class MiniProjeAsalSayi {
	public static void main(String[] args) {
		int sayi = 26;
		
		
		boolean asalmi = true;
		for (int i = 2; i < sayi; i++) {

			if (sayi % i == 0) {
				asalmi = false;

			}
		}
		
		if (asalmi) {
				System.out.println("Sayi asaldir");
			

		}
		else {
			System.out.println("Sayi Asal DeÄŸildir");
		}

	}

}
