package arrays;

import java.util.Iterator;

public class ArraysDemos {
	public static void main(String[] args) {
		String[] ogrenciler = new String[4];
		ogrenciler[0] = "Ezgi";
		ogrenciler[1] = "Can";
		ogrenciler[2] = "Åžeyma";
		ogrenciler[3] = "Zahir";
	for(int i=0;i<ogrenciler.length;i++) {
		
		System.out.println(ogrenciler[i]);
	}
	}

}
