package loops;

public class LoopExamplesDemos {
	public static void main(String[] args) {

		for (int i = 1; i <= 10; i++) {
			System.out.println(i);

		}
		System.out.println("Döngü Bitti");

		for (int j = 1; j <= 10; j += 2) {
			System.out.println(j);

		}
		System.out.println("Döngü Bitti");
	}

}
