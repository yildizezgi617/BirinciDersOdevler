package mukemmelsayi;

public class MukemmelSayiDemo {
	public static void main(String[] args) {
		int sayi = 6;
		int total = 0;
		for (int i = 0; i < sayi; i++) {
			
			if ( sayi % i == 0) {
			

				total = i + total;

			}
			
		}
if(total==sayi) {
	
	System.out.println("mukemmel sayidir");
}
else {
	System.out.println("muk sayi deÄŸil");
}
	}

	}
	
